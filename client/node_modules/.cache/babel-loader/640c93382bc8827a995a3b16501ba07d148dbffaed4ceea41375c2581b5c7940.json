{"ast":null,"code":"import{createSlice}from'@reduxjs/toolkit';import{fetchUser}from'../api/getUser';import{fetchDeleteUser}from'../api/deleteUser';var initialState={data:{memberId:null,email:'',name:'',admin:false}};var userSlice=createSlice({name:'user',initialState:initialState,reducers:{resetUser:function resetUser(state){state.data.memberId=null;state.data.email='';state.data.name='';state.data.admin=false;},setUser:function setUser(state,action){state.data.name=action.payload;}},extraReducers:function extraReducers(builder){builder.addCase(fetchUser.fulfilled,function(state,action){state.data.memberId=action.payload.data.memberId;state.data.email=action.payload.data.email;state.data.name=action.payload.data.name;if(action.payload.data.email===process.env.REACT_APP_ADMIN_ID)state.data.admin=true;})// fetchDeleteUser\n.addCase(fetchDeleteUser.fulfilled,function(state,action){state.data.memberId=null;state.data.email='';state.data.name='';state.data.admin=false;}).addCase(fetchDeleteUser.rejected,function(state,action){state.error=action.error;});}});export default userSlice;var _userSlice$actions=userSlice.actions,resetUser=_userSlice$actions.resetUser,setUser=_userSlice$actions.setUser;export{resetUser,setUser};","map":{"version":3,"names":["createSlice","fetchUser","fetchDeleteUser","initialState","data","memberId","email","name","admin","userSlice","reducers","resetUser","state","setUser","action","payload","extraReducers","builder","addCase","fulfilled","process","env","REACT_APP_ADMIN_ID","rejected","error","_userSlice$actions","actions"],"sources":["/Users/jiinpark/seb44_pre_006/client/src/store/userSlice.js"],"sourcesContent":["\nimport { createSlice } from '@reduxjs/toolkit';\nimport { fetchUser } from '../api/getUser';\nimport { fetchDeleteUser } from '../api/deleteUser';\n\nconst initialState = {\n  data: {\n    memberId: null,\n    email: '',\n    name: '',\n    admin: false,\n  },\n};\n\nconst userSlice = createSlice({\n  name: 'user',\n  initialState,\n  reducers: {\n    resetUser(state) {\n      state.data.memberId = null;\n      state.data.email = '';\n      state.data.name = '';\n      state.data.admin = false;\n    },\n     setUser(state, action) {\n        state.data.name = action.payload;\n     }\n  },\n  extraReducers: (builder) => {\n    builder\n      .addCase(fetchUser.fulfilled, (state, action) => {\n        state.data.memberId = action.payload.data.memberId;\n        state.data.email = action.payload.data.email;\n        state.data.name = action.payload.data.name;\n        if (action.payload.data.email === process.env.REACT_APP_ADMIN_ID)\n          state.data.admin = true;\n      })\n      // fetchDeleteUser\n      .addCase(fetchDeleteUser.fulfilled, (state, action) => {\n        state.data.memberId = null;\n        state.data.email = '';\n        state.data.name = '';\n        state.data.admin = false;\n      })\n      .addCase(fetchDeleteUser.rejected, (state, action) => {\n        state.error = action.error;\n      });\n\n  },\n});\n\nexport default userSlice;\nexport const { resetUser, setUser } = userSlice.actions;\n  \n\n"],"mappings":"AACA,OAASA,WAAW,KAAQ,kBAAkB,CAC9C,OAASC,SAAS,KAAQ,gBAAgB,CAC1C,OAASC,eAAe,KAAQ,mBAAmB,CAEnD,GAAM,CAAAC,YAAY,CAAG,CACnBC,IAAI,CAAE,CACJC,QAAQ,CAAE,IAAI,CACdC,KAAK,CAAE,EAAE,CACTC,IAAI,CAAE,EAAE,CACRC,KAAK,CAAE,KACT,CACF,CAAC,CAED,GAAM,CAAAC,SAAS,CAAGT,WAAW,CAAC,CAC5BO,IAAI,CAAE,MAAM,CACZJ,YAAY,CAAZA,YAAY,CACZO,QAAQ,CAAE,CACRC,SAAS,UAAAA,UAACC,KAAK,CAAE,CACfA,KAAK,CAACR,IAAI,CAACC,QAAQ,CAAG,IAAI,CAC1BO,KAAK,CAACR,IAAI,CAACE,KAAK,CAAG,EAAE,CACrBM,KAAK,CAACR,IAAI,CAACG,IAAI,CAAG,EAAE,CACpBK,KAAK,CAACR,IAAI,CAACI,KAAK,CAAG,KAAK,CAC1B,CAAC,CACAK,OAAO,UAAAA,QAACD,KAAK,CAAEE,MAAM,CAAE,CACpBF,KAAK,CAACR,IAAI,CAACG,IAAI,CAAGO,MAAM,CAACC,OAAO,CACnC,CACH,CAAC,CACDC,aAAa,CAAE,SAAAA,cAACC,OAAO,CAAK,CAC1BA,OAAO,CACJC,OAAO,CAACjB,SAAS,CAACkB,SAAS,CAAE,SAACP,KAAK,CAAEE,MAAM,CAAK,CAC/CF,KAAK,CAACR,IAAI,CAACC,QAAQ,CAAGS,MAAM,CAACC,OAAO,CAACX,IAAI,CAACC,QAAQ,CAClDO,KAAK,CAACR,IAAI,